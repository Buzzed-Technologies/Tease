{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.+\\..+)", "dest": "/public/$1" },
    { "src": "/(login|signup|dashboard|chat|subscription)", "dest": "/public/$1.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
} 